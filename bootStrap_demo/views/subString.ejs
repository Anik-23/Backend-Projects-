<%- include('partials/head.ejs') %> 

    <%- include('partials/navbar.ejs') %> 
   

<div class="container">

    <h1>Browsing the <%= name %> subString </h1>
    <h2><%= description %> </h2>
    <p><%= subscribers %>-Total Subscribers </p>
    <hr>
    <% for(let post of posts){ %> 
        <article>
        <p><%= post.title %>-<b><%= post.author %></b></p>
        <% if(post.img){ %>
            <img src=<%= post.img %> alt="">
            <% } %>  
        </article>
        <hr>
        <% } %> 
</div>

